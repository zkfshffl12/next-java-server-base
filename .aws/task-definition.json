{
    "family": "next-java-server-base-task-family",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::077672914621:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "next-java-server-base-container",
        "image": "077672914621.dkr.ecr.ap-northeast-2.amazonaws.com/next-java-server-base:latest",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/next-java-server-base",
            "awslogs-region": "ap-northeast-2",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }
  